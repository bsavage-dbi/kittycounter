{
  "name": "Kitty Counter",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Count your kitties!",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "cat"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "<all_urls>"],
  "icons": {
    "128": "img/icon128a.png"
  }
}
